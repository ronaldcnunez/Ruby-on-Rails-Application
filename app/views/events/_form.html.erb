<%= form_for @event do |f| %>
  <%= f.label :date %>
  <%= f.text_field :date %>
  <%= f.label :description %>
  <%= f.text_area :description %>
  <%= f.collection_select(:artist_id, Artist.all, :id, :name) %>
  <%= f.collection_select(:venue_id, Venue.all, :id, :name) %>
  <%= f.submit %>
<% end %>
