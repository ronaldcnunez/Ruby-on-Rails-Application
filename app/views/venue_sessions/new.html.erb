<%= render partial: './static/toolbar' %>
    <h1>Venue Login</h1>

    <h3><%= flash[:notice] %></h3>

    <%= form_tag venue_login_path, method: :post do %>
      <%= label_tag "Email" %>
      <%= text_field_tag :email %><br>
      <%= label_tag "Password" %>
      <%= text_field_tag :password %><br>
      <%= submit_tag "Login" %>
    <% end %>

    <%= link_to "Create an Venue Account", new_venue_path %>
