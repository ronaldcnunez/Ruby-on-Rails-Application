<h1>Artist Login</h1>
<%= form_for(:session, url: artist_login_url) do |f| %>

      <%= f.label :email %>
      <%= f.email_field :email %> <br>

      <%= f.label :password %>
      <%= f.password_field :password %> <br>

      <%= f.submit "Log in", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
